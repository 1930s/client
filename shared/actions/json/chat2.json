{
  "prelude": [
    "import * as RPCChatTypes from '../constants/types/rpc-chat-gen'",
    "import * as Types from '../constants/types/chat2'",
    "import HiddenString from '../util/hidden-string'"
  ],
  "actions": {
    "setLoading": {
      "key": "string",
      "loading": "boolean"
    },
    "selectConversation": {
      "conversationIDKey": "?Types.ConversationIDKey",
      "fromUser?": "boolean"
    },
    "badgesUpdated": {
      "conversations": "Array<RPCTypes.BadgeConversationInfo>"
    },
    "setupChatHandlers": {},
    "inboxRefresh": {
      "reason": "string",
      "clearAllData?": "boolean"
    },
    "setInboxFilter": {
      "filter": "string"
    },
    "metaNeedsUpdating": {
      "conversationIDKeys": "Array<Types.ConversationIDKey>",
      "reason": "string"
    },
    "metaHandleQueue": {},
    "metaRequestTrusted": {
      "conversationIDKeys": "Array<Types.ConversationIDKey>"
    },
    "metaUpdateTrustedState": {
      "newState": "Types.MetaTrustedState",
      "conversationIDKeys": "Array<Types.ConversationIDKey>"
    },
    "metasReceived": {
      "metas": "Array<Types.ConversationMeta>"
    },
    "metaReceivedError": {
      "conversationIDKey": "Types.ConversationIDKey",
      "error": "?RPCChatTypes.ConversationErrorLocal",
      "username": "?string"
    },
    "messagesAdd": {
      "fromThreadLoad?": "Types.ConversationIDKey",
      "notify?": "boolean",
      "messages": "Array<Types.Message>"
    },
    "messageEdit": {
      "conversationIDKey": "Types.ConversationIDKey",
      "ordinal": "Types.Ordinal",
      "text": "HiddenString"
    },
    "messagesDelete": {
      "conversationIDKey": "Types.ConversationIDKey",
      "ordinals": "Array<Types.Ordinal>"
    },
    "clearOrdinals": {
      "conversationIDKey": "Types.ConversationIDKey"
    },
    "setSearching": {
      "searching": "boolean"
    },
    "loadMoreMessages": {
      "conversationIDKey": "Types.ConversationIDKey"
    }
  }
}
